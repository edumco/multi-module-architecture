<?xml version="1.0" encoding="utf-8"?><!--
  https://github.com/android/app-bundle-samples/blob/main/DynamicFeatureNavigation/XML/app/src/main/res/navigation/nav_graph.xml
  https://developer.android.com/guide/navigation/navigation-dynamic
  https://developer.android.com/guide/navigation/navigation-deep-link
  -->
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_navigation_graph"
    app:startDestination="@id/main_fragment">

    <fragment
        android:id="@+id/main_fragment"
        android:name="com.example.app.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" />

    <dialog
        android:id="@+id/login_launcher"
        android:name="com.example.login.LoginLauncher"
        android:label="LoginLauncher"
        app:moduleName="ModularFeatureArchitecture.modules.journey.login" >
        <argument
            android:name="login_custom_dependencies"
            android:defaultValue="@null"
            app:argType="com.example.journey.login.LoginCustomDependencies"
            app:nullable="true" />
    </dialog>

</navigation>